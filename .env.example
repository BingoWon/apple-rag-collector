# Apple RAG Collector Configuration
# Copy this file to .env and configure your database settings

DB_HOST=localhost
DB_PORT=5432
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASSWORD=your_database_password
DB_SSL=false

# Application Settings - Modern Optimized Defaults
LOG_LEVEL=info
BATCH_SIZE=25

# Embedding Configuration
EMBEDDING_MODEL=Qwen/Qwen3-Embedding-4B
EMBEDDING_DIM=2560
EMBEDDING_MAX_LENGTH=32000

# Embedding API Configuration
# API Keys are managed in api_keys.txt (one key per line)
EMBEDDING_API_BASE_URL=https://api.siliconflow.cn/v1/embeddings
EMBEDDING_API_TIMEOUT=10

# Telegram Bot Notification Configuration
# Set your Telegram Bot URL for error notifications
TELEGRAM_BOT_URL=https://api.telegram.org/bot<YOUR_BOT_TOKEN>/sendMessage?chat_id=<YOUR_CHAT_ID>
