{
  "name": "apple-rag-collector",
  "version": "2.0.0",
  "description": "Apple Developer Documentation Content Collector for RAG applications - Node.js + PostgreSQL",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "fmt": "prettier --write src/**/*.ts",

    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop apple-rag-collector",
    "pm2:restart": "pm2 restart apple-rag-collector",
    "pm2:logs": "pm2 logs apple-rag-collector"
  },
  "keywords": [
    "apple",
    "documentation",
    "rag",
    "collector",
    "nodejs",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/pg": "^8.15.5",
    "prettier": "^3.3.3",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.5",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "pm2": "^5.3.1"
  }
}
